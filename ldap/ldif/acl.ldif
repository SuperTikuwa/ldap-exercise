dn: olcDatabase={1}mdb,cn=config
changetype: modify
replace: olcAccess
olcAccess: {0}to attrs=userPassword,shadowLastChange
    by self write
    by dn="cn=admin,dc=dev,dc=local" write
    by anonymous auth
    by * none
olcAccess: {1}to *
    by users read
    by dn="cn=admin,dc=dev,dc=local" write
    by * none
